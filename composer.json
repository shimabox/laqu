{
    "name": "shimabox/laravel-query-assertion",
    "description": "Assert DB query executed by laravel.",
    "keywords": ["laravel", "query", "assertion"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/shimabox/laravel-query-assertion",
    "authors": [
        {
            "name": "shimabox",
            "email": "shimabox@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0",
        "jdorn/sql-formatter": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelQueryAssertion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelQueryAssertion\\Test\\": "tests/"
        }
    }
}
